#!groovy
// Check abs3 properties
properties([disableConcurrentBuilds()])

pipeline {
  agent {
    label 'master'
  }
  options {
    buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
	timestamp()
  }
  stages {
    stage("Preparations") {
	  steps {
	    sh 'ssh root@ubs3 \ 'hostname\''
	  }
	}
  }
}